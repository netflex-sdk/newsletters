{
    "name": "netflex/newsletters",
    "description": "Netflex Newsletters library",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Newsletters\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "require": {
        "html2text/html2text": "^4.3",
        "netflex/api": "^6.0",
        "netflex/pages": "^6.0",
        "netflex/query-builder": "^6.0",
        "netflex/support": "^6.0",
        "php": "^8.2",
        "tijsverkoyen/css-to-inline-styles": "^2.2.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Newsletters\\Providers\\NewslettersServiceProvider"
            ]
        }
    }
}
