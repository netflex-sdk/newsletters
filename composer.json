{
    "name": "netflex/newsletters",
    "description": "Netflex Newsletters library",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Netflex\\Newsletters\\": ""
        }
    },
    "replace": {
        "netflex/newsletters": "*"
    },
    "require": {
        "netflex/api": "^6.0",
        "netflex/pages": "^6.0",
        "netflex/query-builder": "^6.0",
        "netflex/support": "^6.0",
        "php": "^8.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Netflex\\Newsletters\\Providers\\NewslettersServiceProvider"
            ]
        }
    }
}
